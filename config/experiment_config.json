      "feature_set": "baseline"
    },
    {
      "claim": "LR recall enhanced",
      "target": 0.68,
      "metric": "recall",
      "model": "LogisticRegression",
      "feature_set": "enhanced"
    },
    {
      "claim": "XGB recall baseline",
      "target": 0.78,
      "metric": "recall",
      "model": "XGBoost",
      "feature_set": "baseline"
    },
    {
      "claim": "XGB recall enhanced",
      "target": 0.82,
      "metric": "recall",
      "model": "XGBoost",
      "feature_set": "enhanced"
    },
    {
      "claim": "CatBoost recall baseline",
      "target": 0.77,
      "metric": "recall",
      "model": "CatBoost",
      "feature_set": "baseline"
    },
    {
      "claim": "CatBoost recall enhanced",
      "target": 0.85,
      "metric": "recall",
      "model": "CatBoost",
      "feature_set": "enhanced"
    },
    {
      "claim": "Rule recall",
      "target": 0.81,
